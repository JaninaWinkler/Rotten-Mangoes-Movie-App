<footer class="footer">
<br>
    <% if current_user %>
        <% if current_user.admin %>
          Welcome Admin User: <%= current_user.full_name %> | 
           <%= link_to "Admin Page", admin_users_path %>  |  <%= link_to "Regular View", movies_path %>  | 
        <% else %>
          Signed in as <%= current_user.full_name %>|
        <% end %>
      <%= link_to "Log out", session_path("current"), method: :delete %>
    <% else %>
      <%= link_to "Log In", new_session_path %> | <%= link_to "Sign Up", new_user_path %> 
    <% end %>
</footer>