<h4>Log In</h4>
<%= form_tag session_path do %>
  <div>
    <%= label_tag "email" %><br/>
    <%= text_field_tag "email" %>
  </div>
  <div>
    <%= label_tag :password %><br/>
    <%= password_field_tag :password %>
  </div>
  <a class="waves-effect waves-light btn"><%= submit_tag "Log In" %></a>
<% end %>
